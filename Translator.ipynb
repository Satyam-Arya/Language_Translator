   "source": [
    "import pandas as pd\n",
    "from deep_translator import GoogleTranslator\n",
    "from tqdm.auto import tqdm\n",
    "from concurrent.futures import ThreadPoolExecutor, as_completed\n",
    "import re\n",
    "\n",
    "# Regex pattern to detect Chinese characters\n",
    "chinese_characters_pattern = re.compile(r'[\\u4e00-\\u9fff]+')\n",
    "\n",
    "# Function to determine if a text contains Chinese characters\n",
    "def contains_chinese(text):\n",
    "    return bool(chinese_characters_pattern.search(text))\n",
    "\n",
    "# Function to translate text if it contains Chinese characters\n",
    "def translate_if_chinese(text):\n",
    "    if pd.isnull(text) or text.strip() == '':\n",
    "        return text\n",
    "\n",
    "    if contains_chinese(text):\n",
    "        try:\n",
    "            # Translate entire text to English\n",
    "            translated = GoogleTranslator(source='auto', target='en').translate(text)\n",
    "            return translated\n",
    "        except Exception as e:\n",
    "            return \"Translation Error\"\n",
    "    else:\n",
    "        return text  # Leave text unchanged if no Chinese is found\n",
    "\n",
    "# Function to translate a Pandas column based on condition\n",
    "def translate_column_based_on_chinese(column):\n",
    "    translated_list = [None] * len(column)  # Initialize a list for translated texts\n",
    "    with ThreadPoolExecutor(max_workers=6) as executor:\n",
    "        # Launch translation tasks in parallel\n",
    "        futures = {executor.submit(translate_if_chinese, text): idx for idx, text in enumerate(column)}\n",
    "\n",
    "        # Set up a progress bar for visual feedback\n",
    "        progress_bar = tqdm(total=len(column), desc=f\"Translating {column.name}\")\n",
    "        \n",
    "        # Retrieve results from completed tasks\n",
    "        for future in as_completed(futures):\n",
    "            idx = futures[future]\n",
    "            try:\n",
    "                translated_list[idx] = future.result()\n",
    "            except Exception as e:\n",
    "                translated_list[idx] = \"Translation Error\"\n",
    "            progress_bar.update(1)  # Update progress bar\n",
    "        \n",
    "        progress_bar.close()  # Close the progress bar\n",
    "    \n",
    "    return translated_list"
   ]
  },
